{
  "productName": "Tablerreur",
  "identifier": "com.tablerreur.desktop",
  "version": "0.1.0",
  "build": {
    "frontendDist": "./frontend"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Tablerreur",
        "width": 1200,
        "height": 800,
        "url": "about:blank"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "binaries/tablerreur-backend-aarch64-apple-darwin/**/*"
    ]
  }
}
